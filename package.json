{
  "name": "webapplicationseed",
  "version": "1.0.0",
  "description": "StepChange Angular2 Application Seed",
  "main": "index.html",
  "scripts": {
    "clean": "node_modules/.bin/rimraf build",
    "pretest": "npm run clean && npm run build",
    "postinstall": "npm run typings",
    "test": "node_modules/.bin/karma start karma.conf.js",
    "typings": "node_modules/.bin/typings install",
    "watch": "node_modules/.bin/tsc --outDir build --watch",
    "tsc": "node_modules/.bin/tsc --outDir build",
    "tslint": "node_modules/.bin/tslint -c tslint.json app/**/*.ts",
    "protractor": "protractor",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "build": "webpack",
    "start": "webpack-dev-server --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "http://fcctfs:8080/tfs/tfssandpit/FrontEndApplications/_git/WebApplicationSeed"
  },
  "author": "StepChange",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "^2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "^0.19.29",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "codelyzer": "0.0.23",
    "html-webpack-plugin": "^2.21.0",
    "jasmine": "2.3.2",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "protractor": "^3.3.0",
    "rimraf": "^2.5.2",
    "ts-loader": "^0.8.2",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
