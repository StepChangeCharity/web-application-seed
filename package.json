{
  "name": "webapplicationseed",
  "version": "1.0.0",
  "description": "StepChange Angular2 Application Seed",
  "main": "index.html",
  "scripts": {
    "preinstall": "npm install -g webdriver-manager rimraf",
    "postinstall": "typings install",
    "webpackserver": "webpack-dev-server --progress --colors --watch --inline",
    "start": "npm run webpackserver -- --config config/webpack.dev.js --hot",
    "start:prod": "npm run webpackserver -- --config config/webpack.prod.js --port 9090 flatten",
    "clean:build": "rimraf -- dist",
    "clean:coverage": "rimraf -- coverage",
    "clean:automation": "rimraf -- automation",
    "clean:documentation": "rimraf -- documentation",
    "clean:dependencies": "npm uninstall rimraf && rimraf -- node_modules typings",
    "clean:install": "npm run clean:dependencies && npm install",
    "pretest": "npm run clean:coverage",
    "test": "karma start config/karma.conf.js",
    "test:debug": "karma start config/karma.conf.js --single-run=false",
    "posttest": "npm run remaphtml && npm run remapteamcity",
    "remaphtml": "remap-istanbul -i coverage/json/coverage-final.json -o coverage -t html",
    "remapteamcity": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/teamcity.txt -t teamcity && type coverage\\teamcity.txt",
    "e2e": "concurrently --success first --kill-others \"npm run jsonserver\" \"protractor config/protractor.conf.js\"",
    "pree2e": "webdriver-manager update --standalone && npm run clean:automation",
    "e2e:dev": "npm run e2e -- --baseUrl=\"http://localhost:3000\"",
    "e2e:prod": "npm run e2e -- --baseUrl=\"http://localhost:9090\"",
    "e2e:release": "npm run e2e -- --baseUrl=\"http://localhost:9999\"",
    "prebuildserver:e2e": "npm run build",
    "buildserver:e2e": "concurrently --success first --kill-others \"npm run serve:release\" \"npm run e2e:release\"",
    "prebuild": "npm run clean:build",
    "build": "webpack --config config/webpack.prod.js",
    "httpserver": "http-server -c-1",
    "serve:release": "ws -p 9999 -d ./dist -s index.html",
    "serve:automation": "npm run httpserver -- -o -p 9998 ./automation",
    "serve:coverage": "npm run httpserver -- -o -p 9997 ./coverage",
    "document": "npm run clean:documentation && groc",
    "jsonserver": "json-server --watch db.json --port 3004"
  },
  "repository": {
    "type": "git",
    "url": "http://fcctfs:8080/tfs/tfssandpit/FrontEndApplications/_git/WebApplicationSeed"
  },
  "author": "StepChange",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@ngrx/core": "^1.0.1",
    "@ngrx/effects": "^2.0.0-beta.3",
    "@ngrx/store": "^2.1.2",
    "@ngrx/store-devtools": "^3.0.2",
    "@ngrx/store-log-monitor": "^3.0.2",
    "core-js": "^2.4.0",
    "ngrx-store-logger": "^0.1.7",
    "raven-js": "3.3.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "codelyzer": "0.0.23",
    "concurrently": "^3.0.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "cucumber": "^1.2.2",
    "cucumber-tsflow": "^1.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "groc": "^0.8.0",
    "html-webpack-plugin": "^2.21.0",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine": "2.3.2",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-teamcity-reporter": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "local-web-server": "^1.2.6",
    "node-sass": "^3.8.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.9.1",
    "protractor": "^3.3.0",
    "protractor-cucumber-framework": "^0.6.0",
    "protractor-jasmine2-html-reporter": "0.0.6",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.1",
    "style-loader": "^0.13.1",
    "tree-kill": "^1.1.0",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.9.1",
    "tslint": "^3.11.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^1.8.10",
    "typings": "^1.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
